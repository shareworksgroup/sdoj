<!-- 编译信息 Modal -->
<div class="modal fade" id="compiler-modal" tabindex="-1" role="dialog" aria-labelledby="compiler-modal-label" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="compiler-modal-label">编译器输出<span data-bind="text: ' - ' + solutionId()"></span></h4>
            </div>
            <div class="modal-body">
                <pre data-bind="text: compilerOutput"></pre>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">
                    关闭
                </button>
            </div>
        </div>
    </div>
</div>