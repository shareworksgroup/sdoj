<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--For more information on log4net config, visit http://logging.apache.org/log4net/release/manual/configuration.html -->
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
   </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <appSettings>
    <add key="serverUrl" value="https://demo.sdcb.in/" />
    <!--<add key="serverUrl" value="http://localhost:54130/" />-->
    <add key="username" value="j" />
    <add key="password" value="A-Pa5sword-That:Never8eenUsed" />
    <add key="vcCommandline" value="&quot;C:\Program Files (x86)\Microsoft Visual Studio 12.0\VC\vcvarsall.bat&quot; amd64" />
    <!--<add key="gccPath" value="C:\MinGW\set_distro_paths.bat"/>-->
    <add key="python3Path" value="C:\Python34\python.exe" />
  </appSettings>
  
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source=|DataDirectory|database.sdf" providerName="System.Data.SqlServerCe.4.0" />
  </connectionStrings>
  <log4net>
    <appender name="Console" type="log4net.Appender.ColoredConsoleAppender">
      <target value="Console.Out" />
      <mapping>
        <level value="DEBUG" />
        <foreColor value="Green" />
      </mapping>
      <mapping>
        <level value="INFO" />
        <foreColor value="Cyan" />
      </mapping>
      <mapping>
        <level value="WARN" />
        <foreColor value="Yellow" />
      </mapping>
      <mapping>
        <level value="Error" />
        <foreColor value="Red, HighIntensity" />
      </mapping>
      <mapping>
        <level value="Fatal" />
        <foreColor value="Red" />
        <backColor value="White" />
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <!-- http://logging.apache.org/log4net/release/sdk/log4net.Layout.PatternLayout.html -->
        <conversionPattern value="%timestamp [%thread %level] %logger{1}: %message%newline" />
      </layout>
    </appender>
    <appender name="File" type="log4net.Appender.RollingFileAppender">
      <file value="log\log.txt" />
      <rollingStyle value="Composite" />
      <!--common-->
      <appendToFile value="true" />
      <preserveLogFileNameExtension value="true" />
      <staticLogFileName value="true" />
      <!--size-->
      <countDirection value="-1" />
      <maximumFileSize value="255KB" />
      <maxSizeRollBackups value="4" />
      <!--date-->
      <datePattern value="yyyyMMdd" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%timestamp [%thread %level] %logger{1}: %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="Console" />
      <appender-ref ref="File" />
    </root>
  </log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
